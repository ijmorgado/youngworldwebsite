<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<title>Mundo Joven - Content Management System</title>
		<meta name="description" content="overview &amp; stats" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<link rel="stylesheet" href="/Content/themes/base/bootstrap.min.css" />
		<link rel="stylesheet" href="/Content/themes/base/bootstrap-responsive.min.css" />
		<link rel="stylesheet" href="/Content/themes/base/font-awesome.min.css" />
		<!--[if IE 7]>
		  <link rel="stylesheet" href="/Content/themes/base/font-awesome-ie7.min.css" />
		<![endif]-->
		<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300" />
		<link rel="stylesheet" href="/Content/themes/base/ace.min.css" />
		<link rel="stylesheet" href="/Content/themes/base/ace-responsive.min.css" />
		<!--[if lte IE 8]>
		  <link rel="stylesheet" href="/Content/themes/base/ace-ie.min.css" />
		<![endif]-->
        <link rel="shortcut icon" href="/Images/static/favicon.png">
   </head>
	<body>
		<div class="navbar navbar-inverse">
			<div class="navbar-inner">
				<div class="container-fluid">
					<a href="#" class="brand">
						<small>
							<i class="icon-cogs"></i>
							Mundo Joven
						</small>
					</a>
					<ul class="nav ace-nav pull-right">
						<li class="light-blue user-profile">
							<a data-toggle="dropdown" href="#" class="user-menu dropdown-toggle">
								<img class="nav-user-photo" src="/Images/static/user.png" alt="User's Photo" />
								<span id="user_info">
									<small>Bienvenido,</small>
									Ivan
								</span>
								<i class="icon-caret-down"></i>
							</a>
							<ul class="pull-right dropdown-menu dropdown-yellow dropdown-caret dropdown-closer" id="user_menu">
								<li>
									<a href="#">
										<i class="icon-off"></i>
										Logout
									</a>
								</li>
							</ul>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<div class="container-fluid" id="main-container">
			<a id="menu-toggler" href="#">
				<span></span>
			</a>
			<div id="sidebar">
				<ul class="nav nav-list">
					<li class="active">
						<a href="index.html">
							<i class="icon-list-alt"></i>
							<span>Notas</span>
						</a>
					</li>
				</ul>
				<div id="sidebar-collapse">
					<i class="icon-double-angle-left"></i>
				</div>
			</div>
			<div id="main-content" class="clearfix">
				<div id="page-content" class="clearfix">
					<div class="page-header position-relative">
						<h1>
							Notas
						</h1>
					</div>
               	<div class="row-fluid">
						@RenderBody()
					</div>
				</div>
			</div>
        </div> 
		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
		<script type="text/javascript">
			window.jQuery || document.write("<script src='/Scripts/jquery-1.9.1.min.js'>"+"<"+"/script>");
		</script>
		<script src="/Scripts/bootstrap.min.js"></script>
		<script src="/Scripts/ace-elements.min.js"></script>
		<script src="/Scripts/ace.min.js"></script>
  </body>
</html>